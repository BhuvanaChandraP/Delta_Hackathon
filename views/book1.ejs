<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        body{
            text-align: center;
            background-color: aqua;
            /* font-size: xx-large; */
        }
        a{
            text-decoration: none;
        }
        #home{
            text-decoration: none;
            font-size: xx-large;
        }
        #new{
            text-decoration: none;
            font-size: xx-large;
        }
    </style>
</head>
<body>
    <%- include('partials/navbar') %>
    <div style="height:100px;"></div>
    <!-- <h1>Hi</h1> -->
    <!-- <%= book1 %>  -->
    <a href = "/book/<%= book1.name %>/page" id = "home">Home</a><br><br>
    <a href = "/new/<%= book1._id %>/page" id = "new">new</a>
    <br><br>
    <!-- <a href = "/book1/page">1</a>
    <a href = "/new/1/page">new</a> -->
    <h1>Pages</h1>
    <% for(let i =0;i<book1.pages.length;i++){ %> 
        <h1><%= book1.pages[i].header %> &nbsp &nbsp &nbsp
            <a href = "/display/<%= book1.pages[i]._id %>">Open</a> &nbsp &nbsp &nbsp
            <a href = "/edit/<%= book1._id %>/<%= book1.pages[i]._id %>">Edit </a> &nbsp &nbsp &nbsp
            <a href = "/delete/<%= book1._id %>/<%= book1.pages[i]._id %>">Delete</a>
        </h1>
    <% } %> 
   
</body>
</html>